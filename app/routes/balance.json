{
  "/fund/token": {
    "POST": {
      "route": "fundToken",
      "validators": {
        "body": {
          "accountAddress": {
            "notEmpty": true,
            "errorMessage": "is missing"
          },
          "tokenAddress": {
            "notEmpty": true,
            "errorMessage": "is missing"
          },
          "originNetwork": {
            "notEmpty": true,
            "errorMessage": "is missing"
          }
        }
      }
    }
  },
  "/fund/native": {
    "POST": {
      "route": "fundNative",
      "validators": {
        "body": {
          "accountAddress": {
            "notEmpty": true,
            "errorMessage": "is missing"
          }
        }
      }
    }
  },
  "/fund/native/status/:id": {
    "GET": {
      "route": "fundNativeStatus"
    }
  },
  "/fund/token/status/:id": {
    "GET": {
      "route": "fundTokenStatus"
    }
  },
  "/balance/native/:accountAddress": {
    "GET": {
      "route": "balanceNative"
    }
  },
  "/balance/token/:accountAddress/:tokenAddress": {
    "GET": {
      "route": "balanceToken"
    }
  },
  "/bonus/token": {
    "POST": {
      "route": "bonusToken",
      "validators": {
        "body": {
          "accountAddress": {
            "notEmpty": true,
            "errorMessage": "is missing"
          },
          "tokenAddress": {
            "notEmpty": true,
            "errorMessage": "is missing"
          },
          "originNetwork": {
            "notEmpty": true,
            "errorMessage": "is missing"
          },
          "bonusInfo": {
            "notEmpty": true,
            "errorMessage": "is missing"
          }
        }
      }
    }
  }
}
